- var n = 1;

div(class='list')
  div(class='list__container')
    div(class='list__inner')
      each item in list
        a(href=`${item.link}` class='list-card')
          span(class='list-card__serial-number')= item.order
          h2(class='list-card__title')= item.title
    include ../components/pagination
